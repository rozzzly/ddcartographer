{
    "name": "ddcartographer",
    "version": "1.0.0",
    "description": "a tool to help generate targeted map/domain files for use with ddrescue",
    "main": "./index.js",
    "module": "./esm/index.js",
    "repository": "https://github.com/rozzzly/dd-cartographer.git",
    "author": "rozzzly",
    "license": "MIT",
    "private": true,
    "scripts": {
        "test": "jest",
        "coverage": "jest --coverage",
        "dev": "jest --watch",
        "build:types": "tsc -p ./tsconfig.dist.json --emitDeclarationOnly",
        "build:esm": "TARGET=esm babel src --out-dir bin/esm --extensions \".ts,.tsx\"",
        "build:cjs": "TARGET=cjs babel src --out-dir bin/cjs --extensions \".ts,.tsx\"",
        "dist": "node ./build-and-package.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@jest/globals": "^26.6.2",
        "@rozzzly/babel-plugin-module-extension-resolver": "^1.0.0-rc.1",
        "@types/common-tags": "^1.8.0",
        "@types/fs-extra": "^9.0.4",
        "@types/node": "^14.14.10",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "common-tags": "^2.0.0-alpha.1",
        "eslint": "^7.18.0",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "jest-cli": "^26.6.3",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "async-line-reader": "^1.0.0",
        "bfj": "^7.0.2",
        "chalk": "^4.1.0",
        "commander": "^7.2.0",
        "execa": "^4.1.0",
        "fs-extra": "^9.0.1",
        "ora": "^5.1.0",
        "pretty-bytes": "^5.6.0",
        "printf": "^0.6.1"
    }
}
